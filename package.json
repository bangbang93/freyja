{
  "name": "freyja",
  "version": "1.2.7",
  "private": true,
  "engines": {
    "node": ">=18.0.0 <23.0.0"
  },
  "scripts": {
    "start": "node packages/server/dist/main.js",
    "dev": "npm -w @bangbang93/freyja-server run dev",
    "pm2": "pm2 startOrReload process.json",
    "build": "npm -ws run build --if-present",
    "test": "npm run lint && npm run build",
    "lint": "eslint .",
    "snyk-protect": "snyk protect",
    "migrate": "node packages/server/dist/script/migrate.js",
    "update-browserlist-db": "npx update-browserslist-db@latest"
  },
  "dependencies": {
    "mongoose": "^7.8.4",
    "snyk": "^1.1297.3"
  },
  "devDependencies": {
    "@bangbang93/eslint-config-recommended": "^2.0.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-vue": "^10.0.0",
    "release-it": "^19.0.4",
    "shelljs": "^0.8.5",
    "typescript": "^5.2.2",
    "vue-eslint-parser": "^10.2.0"
  },
  "snyk": true,
  "workspaces": [
    "packages/*"
  ]
}
